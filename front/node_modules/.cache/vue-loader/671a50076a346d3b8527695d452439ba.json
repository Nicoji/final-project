rands\" :key=\"index\" @click=\"selectBrand\">{{ brand }}</li>\n        </ul>\n      </div>\n    </div>\n    <div id=\"show-filter\">\n      <p>Showing filters</p>\n      <div id=\"show-sub-categories\">Sub category filter is {{ subCat }} </div>\n      <div id=\"show-price\">Price filter is {{ price }} </div>\n      <div id=\"show-label\">Label filter is {{ label }}  </div>\n      <div id=\"show-brand\">Brand filter is {{ brand }} </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Filters\",\n  data() {\n    return {\n      subCat: \"\",\n      price: 50,\n      label: \"\",\n      brand: \"\",\n      subCategories: [\"Jessicaaaaaaaaaaaaaaaaaaaa\", \"Julien\", \"Laura\", \"Niko\", \"Thibault\", \"Bebew\", \"Manon\", \"Carla\", \"Maeva\", \"Benjamin\", \"Oc√©ane\" ],\n      labels: [\n        { name: \"Natoo\" },\n        { name: \"Norman\" },\n        { name: \"McFly\" },\n        { name: \"Tibo\" },\n        { name: \"Michou\" },\n      ],\n      brands: [\"Toyota\", \"Mitsubishi\", \"Renault\", \"Audi\", \"Smart\"],\n    };\n  },\n  methods:{\n    selectSubCat(event) {\n      if (this.subCat == event.target.innerText) {\n        this.subCat = \"\";\n        event.target.style.backgroundColor = \"\";\n      } else {\n        this.subCat = event.target.innerText;\n        event.target.style.backgroundColor = \"lightgreen\";\n      }\n      console.log(event.target);\n    },\n    selectLabel(event) {\n      this.label = event.target.innerText;\n    },\n    selectBrand(event) {\n      this.brand = event.target.innerText;\n    },\n  },\n  components: {},\n};\n</script>\n\n<style>\n#filters {\n  border: solid 1px blue;\n  width: max-content;\n}\np {\n  border: solid 1px red;\n}\nli {\n  border: dotted 2px green;\n  text-align: left;\n  padding : 2px;\n  margin: 1px;\n}\n\nli:hover {\n  background-color: lightblue;\n  cursor: pointer;\n}\n</style>"]}]}