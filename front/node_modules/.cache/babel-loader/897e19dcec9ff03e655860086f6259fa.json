 ],\n      brands: [\"Toyota\", \"Mitsubishi\", \"Renault\", \"Audi\", \"Smart\"],\n    };\n  },\n  methods:{\n    selectSubCat(event) {\n      if (this.subCat == event.target.innerText) {\n        this.subCat = \"\";\n        event.target.style.backgroundColor = \"\";\n      } else {\n        this.subCat = event.target.innerText;\n        event.target.style.backgroundColor = \"lightgreen\";\n      }\n      console.log(event.target);\n    },\n    selectLabel(event) {\n      this.label = event.target.innerText;\n    },\n    selectBrand(event) {\n      this.brand = event.target.innerText;\n    },\n  },\n  components: {},\n};\n</script>\n\n<style>\n#filters {\n  border: solid 1px blue;\n  width: max-content;\n}\np {\n  border: solid 1px red;\n}\nli {\n  border: dotted 2px green;\n  text-align: left;\n  padding : 2px;\n  margin: 1px;\n}\n\nli:hover {\n  background-color: lightblue;\n  cursor: pointer;\n}\n</style>"],"sourceRoot":"src/views"}]}